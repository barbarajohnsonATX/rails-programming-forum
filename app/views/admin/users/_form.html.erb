<%= form_for([:admin, @user]) do |f|%>
    <%= render partial: 'layouts/errors', locals: {object:  @user} %>

    <div class="form-group">
        <%= f.label :username%> <br/>
        <%= f.text_field :username, class: "form-control" %> <br/>
    </div>


    <div class="form-group">
        <%= f.label :email%> <br/>
        <%= f.email_field :email, class: "form-control" %> <br/>
    </div>

    <br/>
  
    <%= f.submit submit_text, class: 'btn btn-success' %>
 
<% end %>